<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.yulgok.YulgokFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_yulgok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="60dp" />

    <ImageView
        android:id="@+id/image_yulgokbuilding"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:scaleType="fitXY"
        android:src="@drawable/yulgok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.434"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/todo" />


    <TextView
        android:id="@+id/text_yulgok"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.268" />

    <ImageView
        android:id="@+id/image_yulgokline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        android:scaleType="fitXY"
        android:src="@drawable/textline"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_yulgok"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok1f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_1f"
        android:src="@drawable/yulgok1f_image"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/image_yulgokline"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok2f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_3f"
        android:src="@drawable/yulgok2f_image"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/yulgok1f"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok3f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_4f"
        android:src="@drawable/yulgok3f_image"
        app:layout_constraintHorizontal_bias="0.176"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/yulgok2f"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok4f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_5f"
        android:src="@drawable/yulgok4f_image"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/yulgok3f"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok5f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_6f"
        android:src="@drawable/yulgok5f_image"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/yulgok4f"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok6f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_7f"
        android:src="@drawable/yulgok6f_image"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/yulgok5f"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/yulgok7f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/car_7f"
        android:src="@drawable/yulgok7f_image"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@id/guideline_start_yulgok"
        app:layout_constraintTop_toBottomOf="@id/yulgok6f"
        tools:ignore="MissingConstraints" />


    <FrameLayout
        android:id="@+id/fragment_container_yulgok"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>